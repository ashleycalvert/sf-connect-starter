SNOWFLAKE_ACCOUNT=your-account-identifier
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=YOUR_DATABASE
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_ROLE=YOUR_ROLE

# Authentication Method (sso or keypair)
AUTH_METHOD=sso
SSO_USERNAME=your-username
SSO_PASSWORD=your-password

# Key Pair Authentication (when AUTH_METHOD=keypair)
# AUTH_METHOD=keypair
KEYPAIR_USERNAME=your-username
PRIVATE_KEY_PATH=./keys/snowflake_private_key.pem
PRIVATE_KEY_PASSPHRASE=your-passphrase

# SSO Configuration (when AUTH_METHOD=sso)
SNOWFLAKE_AUTHENTICATOR=externalbrowser

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true
LOG_LEVEL=INFO